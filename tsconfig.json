{
	"compilerOptions": {
		"module": "Node16",
		"target": "ES2024",
		"lib": [
			"ES2024"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"outDir": "out",
		"skipLibCheck": true,
		"esModuleInterop": true,
		"resolveJsonModule": true
	},
	"exclude": [
		"node_modules",
		"out",
		"docs"
	]
}
